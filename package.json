{
  "name": "interlude-cast-pro",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down -v",
    "db:migrate": "pnpm --filter api prisma:migrate",
    "db:seed": "pnpm --filter api prisma:seed",
    "db:start": "docker compose exec api npx prisma migrate dev --name init && docker compose exec api pnpm prisma:seed"
  },
  "devDependencies": {
    "turbo": "^2.1.0",
    "typescript": "^5.6.2"
  }
}
